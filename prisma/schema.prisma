generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = "postgresql://tiago:1234@localhost:5432/codiecode"
}

model Category {
  id    Int         @id @default(autoincrement())
  title String
  url   String

  parentId Int?
}


model User {
  id Int @id @default(autoincrement())

  email    String @unique
  password String

  name String
}
